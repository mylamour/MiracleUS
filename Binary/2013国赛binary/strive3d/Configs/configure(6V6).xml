<?xml version="1.0" encoding="UTF-8"?>
<Strive id="Testing Version" xmlns:xi="http://www.w3.org/2003/XInclude">
	<!--
	<xi:include href="Net.xml"/>
	<xi:include href="Logger.xml"/>
	<xi:include href="PathPlanning.xml"/>
	-->
	
	<server>
		<host>localhost</host>
		<port>3100</port>
	</server>
	
	<Logger>
		<MainLog>True</MainLog>
		<ParserLog>True</ParserLog>
		<ActionLog>True</ActionLog>
		<BehaviorControlLog>True</BehaviorControlLog>
		<TrainerLog>False</TrainerLog>
	</Logger>
	
	<Module>
		<!--Choose from walkPathPlanningControl=2,naoBehaviorControl=1,strive3DBehaviorControl=0-->
		<BehaviorControl>0</BehaviorControl>
		<!--Choose from naoActionControl=1-->
		<ActionControl>1</ActionControl>
		<!--Choose from naoWalkingEngine=1-->
		<WalkingEngine>1</WalkingEngine>
		<!--Choose from naoGetupEngine=1,strive3DGetupEngine=0-->
		<GetupEngine>1</GetupEngine>
		<!--Choose from naoHeadControl=1-->
		<HeadControl>1</HeadControl>
		<!--Choose from naoKickEngine=1-->
		<KickEngine>1</KickEngine>
		<!--Choose from naoSpecialAction=1-->
		<SpecialAction>1</SpecialAction>
		
	</Module>
	
	<!--Set True to play the match. Set false to debug the program-->
	<PlayMatch>True</PlayMatch>
	
	<TestWalk>False</TestWalk>
	
	<AdvancedAgent>
		<MatchPlan>False</MatchPlan><!--MatchDecision has the priority.-->
		<TestPlan>False</TestPlan>
		<PathPlanning>
			<TestPathPlanning>True</TestPathPlanning>
			<!-- Common Simulation Arguments -->
			<simCycleMax>1000</simCycleMax>
			<matchSimCycleMax>10</matchSimCycleMax>
			<massFactor>2</massFactor><!--The larger the massFactor is, the harder the robot turns.-->
			<simStep>0.2</simStep><!--<simStep>0.2</simStep> original stable data-->
			<mySpeedMax>2.0</mySpeedMax>
			<virtualDestAngle>20.0</virtualDestAngle>
			<!--Destination factor-->
			<destDistThreshold>1</destDistThreshold><!--This is the threshold of distance from Agent to destination-->
			<destForce>10.0</destForce><!-- Force generated by destination is not related to distance -->
			<!--Opponent factor-->
			<oppDistFactor>-1</oppDistFactor><!--<oppDistFactor>-200</oppDistFactor> original stable data-->
			<oppSize>0.2</oppSize>
			<oppForceIndex>-5</oppForceIndex>
		
			<!-- Geometry Arguments -->
			<MyStartPos><!--<MyStartPos><x>1.0</x><y>7.0</y><isValid>True</isValid></MyStartPos>-->
				<x>-10.0</x>
				<y>0.0</y>
				<isValid>False</isValid>
			</MyStartPos>
			<MyStartVel><!--<MyStartVel><x>2.0</x><y>0</y><isValid>True</isValid></MyStartPos>-->
				<x>0</x>
				<y>0</y>
				<isValid>True</isValid>
			</MyStartVel>
			<DestPos>
				<x>10.0</x>
				<y>0.0</y>
				<isValid>True</isValid>
			</DestPos>
			<BallPos>
				<x>12.0</x>
				<y>0.0</y>
				<isValid>False</isValid>
			</BallPos>
		
			<hasOpp>True</hasOpp>
			<OppPos ID = "1">
				<x>-8.0</x>
				<y>-0.0</y>
				<isValid>True</isValid>
			</OppPos>
			<OppPos ID = "2">
				<x>-3.0</x>
				<y>1.0</y>
				<isValid>True</isValid>
			</OppPos>
			<OppPos ID = "3">
				<x>5.0</x>
				<y>0.0</y>
				<isValid>True</isValid>
			</OppPos>
			<OppPos ID = "4">
				<x>8.0</x>
				<y>0.0</y>
				<isValid>True</isValid>
			</OppPos>
		</PathPlanning>
	</AdvancedAgent>
	<WorldModel>
		<FieldInfo>
			<FieldSize>
				<FieldLength>18.0</FieldLength>
				<FieldWidth>12.0</FieldWidth>
				<FieldHeight>40.0</FieldHeight>
			</FieldSize>
			<GoalSize>
				<GoalWidth>2.1</GoalWidth>
				<GoalDepth>0.6</GoalDepth>
				<GoalHeight>0.8</GoalHeight>
			</GoalSize>
			<PenaltySize>
				<PenaltyLength>1.8</PenaltyLength>
				<PenaltyWidth>3.9</PenaltyWidth>
			</PenaltySize>
			<FreeKickDistance>1.3</FreeKickDistance>
			<FreeKickMoveDist>1.5</FreeKickMoveDist>	
		</FieldInfo>
	</WorldModel>
	<TeamFormation>
		<TmmStart>
			<Player1>-8 0 10</Player1>	<!-- Goalie -->
			<Player2>-0.5 -0.3 10</Player2>	<!-- forward -->
			<Player3>-0.4 1 10</Player3>	<!-- forward -->
			<Player4>-3 0 10</Player4>	<!-- center -->
			<Player5>-6 -1 10</Player5>	<!-- guard -->
			<Player6>-5.5 0.8 10</Player6>	<!-- guard -->
		</TmmStart>
		<OppStart>
			<Player1>-8 0 10</Player1>	<!-- Goalie -->
			<Player2>-1.8 1.5 10</Player2>	<!-- forward -->
			<Player3>-1.5 -1.5 10</Player3>	<!-- forward -->
			<Player4>-2 0 10</Player4>	<!-- center -->
			<Player5>-6 -1 10</Player5>	<!-- guard -->
			<Player6>-5.5 0.8 10</Player6>	<!-- guard -->
		</OppStart>
	</TeamFormation>
	<WalkingEngine>
		<Golie>
			<WalkingSlowGain>0.08</WalkingSlowGain>
			<Tc>0.14</Tc>
		</Golie>
		<OtherPlayer>
			<WalkingSlowGain>0.08</WalkingSlowGain>
			<Tc>0.16</Tc>
		</OtherPlayer>
	</WalkingEngine>
</Strive>
